/* Wap a Program to Print this Pattern 
1
22
333
4444
55555 */
#include <stdio.h>

int main() {
    int rows; // enter any no of row 

    printf("Enter the number of rows: ");
    scanf("%d", &rows);

    for (int i = 1; i <= rows; i++) { //Row
        for (int j = 1; j <= i; j++) { //Column
            printf("%d", i);
        }
        printf("\n");
    }

    return 0;
}
